{
  "name": "react-dom-lite",
  "version": "0.0.1",
  "description": "Custom react renderer which is a light weight implementation of React DOM",
  "main": "lib/react-dom-lite.cjs.js",
  "scripts": {
    "test:app": "webpack-dev-server --config tests/app/webpack.config.js",
    "rollup": "rollup -c ./scripts/rollup/rollup.config.js",
    "dev:lib": "node ./scripts/watcher",
    "build:lib": "node ./scripts/build/lib-bundle",
    "build": "ENTRY=src/index.js  DEST=lib/react-dom-lite.cjs.js yarn rollup --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prometheansacrifice/react-dom-lite.git"
  },
  "keywords": [
    "React",
    "ReactDOM",
    "Custom",
    "React",
    "renderers"
  ],
  "author": "Manas Jayanth",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prometheansacrifice/react-dom-lite/issues"
  },
  "homepage": "https://github.com/prometheansacrifice/react-dom-lite#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "flow": "^0.2.3",
    "react-dom": "^16.2.0",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-reconciler": "^0.6.0"
  }
}
